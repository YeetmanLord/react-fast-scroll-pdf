{
  "name": "react-fast-scroll-pdf",
  "version": "1.1.1",
  "description": "Image-based PDF viewer using pdfjs",
  "author": "wearemothership",
  "license": "MIT",
  "keywords": [
    "react",
    "pdf",
    "scroll",
    "ui",
    "react-component",
    "pdfjs",
    "pdf.js"
  ],
  "repository": "wearemothership/react-fast-scroll-pdf",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rimraf ./dist && rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rimraf ./dist && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist && rollup -c -w ",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "example": "npm run build && cd example && npm install && npm start"
  },
  "dependencies": {
    "html-react-parser": "^3.0.4",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "pdfjs-dist": "^2.16.105"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-eslint": "^9.0.0",
    "@rollup/plugin-image": "^3.0.0",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.21",
    "@types/react-html-parser": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "gh-pages": "^4.0.0",
    "nan": "^2.17.0",
    "postcss": "^8.4.17",
    "react": "~18.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-runtime"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.module.js"
    }
  }
}
